<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"  "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
<bean id="student" class="com.Student">
		<property name="studentData" ref="studentData"></property>
	</bean>
	<bean id = "studentData" class="com.StudentData">
		<property name="firstName" value="James"></property>
		<property name="lastName" value="Bond"></property>
		<property name="address" value="Byculla"></property>
	</bean>
	
	

</beans>
