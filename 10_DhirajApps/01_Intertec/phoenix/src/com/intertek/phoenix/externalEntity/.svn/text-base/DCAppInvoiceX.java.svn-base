package com.intertek.phoenix.externalEntity;

import java.io.File;

/**
 * Object to send to ESB
 * 
 * @author Eric.Nguyen
 */
public class DCAppInvoiceX {
    private String projectNumber;
    private String orderNumber;
    private String lineItemNumber;
    private boolean invoiceFlag;
    private File invoiceFile;

    public String getProjectNumber() {
        return projectNumber;
    }

    public void setProjectNumber(String projectNumber) {
        this.projectNumber = projectNumber;
    }

    public String getOrderNumber() {
        return orderNumber;
    }

    public void setOrderNumber(String orderNumber) {
        this.orderNumber = orderNumber;
    }

    public String getLineItemNumber() {
        return lineItemNumber;
    }

    public void setLineItemNumber(String lineItemNumber) {
        this.lineItemNumber = lineItemNumber;
    }

    public boolean isInvoiceFlag() {
        return invoiceFlag;
    }

    public void setInvoiceFlag(boolean invoiceFlag) {
        this.invoiceFlag = invoiceFlag;
    }

    public File getInvoiceFile() {
        return invoiceFile;
    }

    public void setInvoiceFile(File invoiceFile) {
        this.invoiceFile = invoiceFile;
    }
}
