package com.intertek.phoenix.externalEntity;

/**
 * @author Eric.Nguyen
 */
public class DCAppBillableLineItemX {
    private String lineItemNumber;
    private String lineItemDescription;
    private int billableItemQuantity;
    private double invoiceAmount;

    public String getLineItemNumber() {
        return lineItemNumber;
    }

    public void setLineItemNumber(String lineItemNumber) {
        this.lineItemNumber = lineItemNumber;
    }

    public String getLineItemDescription() {
        return lineItemDescription;
    }

    public void setLineItemDescription(String lineItemDescription) {
        this.lineItemDescription = lineItemDescription;
    }

    public int getBillableItemQuantity() {
        return billableItemQuantity;
    }

    public void setBillableItemQuantity(int billableItemQuantity) {
        this.billableItemQuantity = billableItemQuantity;
    }

    public double getInvoiceAmount() {
        return invoiceAmount;
    }

    public void setInvoiceAmount(double invoiceAmount) {
        this.invoiceAmount = invoiceAmount;
    }
}
