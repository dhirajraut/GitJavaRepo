package com.intertek.phoenix.web.controller.job;

import com.intertek.entity.Test;
import com.intertek.phoenix.common.Form;

/**
 * Form object which represents each test row on 'Add Test' screen
 * @author rautsmit
 *
 */
public class TestSearchResultRow {

    private Test test;
    private boolean selected;
    private Double qty = 1.0;

    public TestSearchResultRow(Test test) {
        this.test = test;
    }

    public boolean getSelected() {
        return selected;
    }

    public void setSelected(boolean selected) {
        this.selected = selected;
    }

    public Test getTest() {
        return test;
    }
    public Double getQty()
    {
      return qty;
    }

    public void setQty(Double qty)
    {
      this.qty = qty;
    }
}
