package com.intertek.domain;

import java.util.List;

import com.intertek.entity.*;
import com.intertek.util.*;
import com.intertek.calculator.*;
import com.intertek.servicetype.*;

public class VesselTypeExt
{
  private boolean selected = true;
  private boolean newlyAdded;
  private String defaultDescription;
  private String customDescription;

  private VesselType vesselType;
  private VesselType oldVesselType;

  private RBExt rbExt;

  // used in contract builder - inspection tab
  private List pbInspectionRateList;
  private List contractInspectionRateExtList;

  private List deletedInspectionRateExtList;

  private boolean displayAll = true;
  private int rowsToAdd = 1;

  private double vesselMax;

  private InspectionRateExt towMaxInspectionRateExt;

  private boolean checkAll;

  private int inspectionRateTab;

  public VesselTypeExt()
  {
  }

  public boolean getSelected()
  {
    return selected;
  }

  public void setSelected(boolean selected)
  {
    this.selected = selected;
  }

  public boolean getNewlyAdded()
  {
    return newlyAdded;
  }

  public void setNewlyAdded(boolean newlyAdded)
  {
    this.newlyAdded = newlyAdded;
  }

  public String getDefaultDescription()
  {
    return defaultDescription;
  }

  public void setDefaultDescription(String defaultDescription)
  {
    this.defaultDescription = defaultDescription;
  }

  public String getCustomDescription()
  {
    return customDescription;
  }

  public void setCustomDescription(String customDescription)
  {
    this.customDescription = customDescription;
  }

  public void setVesselType(VesselType vesselType)
  {
    this.vesselType = vesselType;
  }

  public VesselType getVesselType()
  {
    return vesselType;
  }

  public void setOldVesselType(VesselType oldVesselType)
  {
    this.oldVesselType = oldVesselType;
  }

  public VesselType getOldVesselType()
  {
    return oldVesselType;
  }

  public RBExt getRbExt()
  {
    return rbExt;
  }

  public void setRbExt(RBExt rbExt)
  {
    this.rbExt = rbExt;
  }

  public List getPbInspectionRateList()
  {
    return pbInspectionRateList;
  }

  public void setPbInspectionRateList(List pbInspectionRateList)
  {
    this.pbInspectionRateList = pbInspectionRateList;
  }

  public List getContractInspectionRateExtList()
  {
    return contractInspectionRateExtList;
  }

  public void setContractInspectionRateExtList(List contractInspectionRateExtList)
  {
    this.contractInspectionRateExtList = contractInspectionRateExtList;
  }

  public List getDeletedInspectionRateExtList()
  {
    return deletedInspectionRateExtList;
  }

  public void setDeletedInspectionRateExtList(List deletedInspectionRateExtList)
  {
    this.deletedInspectionRateExtList = deletedInspectionRateExtList;
  }

  public void setDisplayAll(boolean displayAll)
  {
    this.displayAll = displayAll;
  }

  public boolean getDisplayAll()
  {
    return displayAll;
  }

  public void setRowsToAdd(int rowsToAdd)
  {
    this.rowsToAdd = rowsToAdd;
  }

  public int getRowsToAdd()
  {
    return rowsToAdd;
  }

  public void setVesselMax(double vesselMax)
  {
    this.vesselMax = vesselMax;
  }

  public double getVesselMax()
  {
    return vesselMax;
  }

  public void setTowMaxInspectionRateExt(InspectionRateExt towMaxInspectionRateExt)
  {
    this.towMaxInspectionRateExt = towMaxInspectionRateExt;
  }

  public InspectionRateExt getTowMaxInspectionRateExt()
  {
    return towMaxInspectionRateExt;
  }

  public boolean getCheckAll()
  {
    return checkAll;
  }

  public void setCheckAll(boolean checkAll)
  {
    this.checkAll = checkAll;
  }

  public int getInspectionRateTab()
  {
    return inspectionRateTab;
  }

  public void setInspectionRateTab(int inspectionRateTab)
  {
    this.inspectionRateTab = inspectionRateTab;
  }
}
