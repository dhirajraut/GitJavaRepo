package com.intertek.domain;

import org.springmodules.validation.bean.conf.loader.annotation.handler.CascadeValidation;

import com.intertek.entity.CustAddrSeq;
import com.intertek.entity.CustAddress;

public class AddCustomerAddress
{
  @CascadeValidation
 
  private CustAddrSeq custAddrSeq;
  private CustAddress[] custAddresses;

   private int custAddressCount;
   private String addOrDeleteCustAddress;
   private int custAddressIndex;
   private String primaryBillToAddr;
   private String primarySoldToAddr;

	public void setCustAddrSeq(CustAddrSeq custAddrSeq)
  {
    this.custAddrSeq = custAddrSeq;
  }

  public CustAddrSeq getCustAddrSeq()
  {
    return custAddrSeq;
  }

  public CustAddress[] getCustAddresses()
  {
    return custAddresses;
  }

  public void setCustAddresses(CustAddress[] custAddresses)
  {
    this.custAddresses = custAddresses;
  }



  public int getCustAddressCount()
	  {
		return custAddressCount;
	  }

	  public void setCustAddressCount(int custAddressCount)
	  {
		this.custAddressCount = custAddressCount;
	  }

	  public String getAddOrDeleteCustAddress()
	  {
			return addOrDeleteCustAddress;
		}

		public void setAddOrDeleteCustAddress(String addOrDeleteCustAddress)
		{
			this.addOrDeleteCustAddress = addOrDeleteCustAddress;
		}

		public int getCustAddressIndex()
		{
			return custAddressIndex;
		}

		public void setCustAddressIndex(int custAddressIndex)
		{
			this.custAddressIndex = custAddressIndex;
		}
	  public String getPrimaryBillToAddr()
	  {
			return primaryBillToAddr;
		}

		public void setPrimaryBillToAddr(String primaryBillToAddr)
		{
			this.primaryBillToAddr = primaryBillToAddr;
		}

	  public String getPrimarySoldToAddr()
	  {
			return primarySoldToAddr;
		}

		public void setPrimarySoldToAddr(String primarySoldToAddr)
		{
			this.primarySoldToAddr = primarySoldToAddr;
		}

}
