<script>
  function submitParentForm()
  {
     top.document.forms[0].contractIndex.value = '${requestScope.index}';
     top.document.forms[0].messageKey.value = '${requestScope.messageKey}';
     top.document.forms[0].submit();
  }
  
  submitParentForm(); 
  
  top.closeservices('${requestScope.index}');
</script>
