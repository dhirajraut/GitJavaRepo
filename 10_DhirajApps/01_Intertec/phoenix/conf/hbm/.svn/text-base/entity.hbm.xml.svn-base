      <column name="DEFAULT_BU"/>
    </property>

    <property name="handleVAT" type="java.lang.Boolean">
      <column name="HANDLE_VAT"/>
    </property>

   <property name="defaultBuInd" type="java.lang.Boolean">
      <column name="DEFAULT_BU_IND"/>
    </property>

