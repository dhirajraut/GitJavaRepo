<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.intertek.entity">
  <meta attribute="extra-import">org.springmodules.validation.bean.conf.loader.annotation.handler.*</meta>
  <class name="JobManualTest" table="JOB_M_TESTS">
    <id name="id" type="long" unsaved-value="0">
      <column name="JOB_M_TEST_ID" sql-type="NUMBER" />
      <generator class="sequence">
        <param name="sequence">JOB_M_TEST_SEQ</param>
      </generator>
    </id>
    <many-to-one name="jobProdSample" class="JobProdSample">
      <column name="ITS_SAMP_SEQ_ID" index="JOB_M_TESTS_index_0" />
      <column name="ITS_PROD_SEQ_NUM" index="JOB_M_TESTS_index_0" />
      <column name="JOB_NUMBER" index="JOB_M_TESTS_index_0" />
      <column name="ITS_LINKED_VSL_ROW" index="JOB_M_TESTS_index_0" />
    </many-to-one>
    <property name="testId" type="string">
      <meta attribute="scope-field">@NotBlank @Length(min=0, max=96) private</meta>
      <column name="TEST_ID" length="96" />
    </property>
    <property name="testDescription" type="string">
      <meta attribute="scope-field">@Length(min=0, max=762) private</meta>
      <column name="TEST_DESCR" length="762" />
    </property>
    <property name="quantity" type="java.lang.Double">
      <column name="QUANTITY" sql-type="NUMBER(8, 2)" />
    </property>
    <property name="ot" type="java.lang.Boolean">
      <column name="OT" />
    </property>
    <property name="unitPrice" type="java.lang.Double">
      <column name="CFG_UNIT_PRICE" sql-type="NUMBER(16, 6)" />
    </property>
    <property name="lineDescription" type="string">
      <meta attribute="scope-field">@Length(min=0, max=254) private</meta>
      <column name="LINE_DESCRIPTION" length="254" />
    </property>
    <property name="contractRefNo" type="string">
      <meta attribute="scope-field">@Length(min=0, max=45) private</meta>
      <column name="CONTRACT_REF_NO" length="45" />
    </property>
    <property name="discount" type="java.lang.Double">
      <column name="DISCOUNT" sql-type="NUMBER(10, 4)" />
    </property>
    <property name="totalPrice" type="java.lang.Double">
      <column name="TOTAL_PRICE" sql-type="NUMBER(30, 2)" />
    </property>
  </class>
</hibernate-mapping>

