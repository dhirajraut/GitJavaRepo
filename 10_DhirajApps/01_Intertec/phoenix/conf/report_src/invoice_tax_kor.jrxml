	<queryString><![CDATA[SELECT  
        jc.JOB_NUMBER,
        jci.INVOICE,
        jo.BU_NAME,
        jo.branch_name,
        jc.INVOICE_DATE,
        jc.INVOICE_DESCR,
        jc.INV_AMT,
        sum(il.VAT_AMT) AS "TAX AMOUNT",           
        jci.VAT_REGISTRATION_ID,
        buvl.VAT_REGISTRATION_ID AS "SELLER VAT REG ID",
        bul.address1, 
        bul.address2, 
        bul.address3, 
        bul.address4, 
        bul.business_item AS "BU BUSINESS ITEM", 
        bul.business_type AS "BU BUSINESS TYPE", 
        bul.city, 
        bul.company_desc, 
        bul.county,
	bul.postal AS "BU POSTAL",
	bul.REP_DIRECTOR AS "BU DIRECTOR", 
        bl.COMPANY_DESC AS "BRANCH CO DESC", 
        jc.CUST_CODE,
        cl.NAME AS "CU NAME",
        cl.REP_DIRECTOR,
        cl.BUSINESS_TYPE AS "CU BUSINESS TYPE",
        cl.BUSINESS_ITEM AS "CU BUSINESS ITEM",
        cal.ADDRESS1 AS "CU ADD1",
        cal.ADDRESS2 AS "CU ADD2",
        cal.ADDRESS3 AS "CU ADD3",
        cal.ADDRESS4 AS "CU ADD4",
        cal.CITY AS "CU CITY",
        cal.COUNTY AS "CU COUNTY",
        cal.POSTAL AS "CU POSTAL"
        
        
      FROM PHOENIX.JOB_CONTRACTS jc, PHOENIX.JOB_CONTRACT_INVOICE jci, PHOENIX.INVOICE_LINES il, 
           PHOENIX.JOB_ORDERS jo, PHOENIX.BUS_UNIT_VAT_LOC buvl, PHOENIX.BUSINESS_UNITS_LANG bul, PHOENIX.BRANCHES_LANG bl,
           PHOENIX.CUSTOMERS_LANG cl, PHOENIX.CUST_ADDRESSES ca, PHOENIX.CUST_ADDRESSES_LANG cal
    
      WHERE jc.JOB_CONTRACT_ID = jci.JOB_CONTRACT_ID
        AND jci.JOB_CONTRACT_INVOICE_ID = il.JOB_CONTRACT_INVOICE_ID
	AND jc.JOB_NUMBER = jo.JOB_NUMBER
	AND jo.BU_NAME = buvl.BU_NAME
	AND jci.BUSINESS_UNITS_COUNTRY_CODE = buvl.COUNTRY_CODE
  AND jo.BU_NAME = bul.BU_NAME
  AND jo.BRANCH_NAME = bl.BRANCH_NAME
  AND jc.CUST_CODE = cl.CUST_CODE
AND JCI.JOB_CONTRACT_INVOICE_ID = $P{Invoice_Id}
  AND jc.CUST_CODE = ca.CUST_CODE
  AND jc.CUST_LOCATION_NUMBER = ca.LOCATION_NUMBER
  AND ca.CUST_ADDRESS_ID = cal.CUST_ADDRESS_ID
  
GROUP BY
jc.JOB_NUMBER,
        jc.JOB_NUMBER,
        jci.INVOICE,
        jo.BU_NAME,
        jo.branch_name,
        jc.INVOICE_DATE,
        jc.INVOICE_DESCR,
        jc.INV_AMT,
        jci.VAT_REGISTRATION_ID,
        buvl.VAT_REGISTRATION_ID,
        bul.address1, 
        bul.address2, 
        bul.address3, 
        bul.address4, 
        bul.business_item, 
        bul.business_type, 
        bul.city, 
        bul.company_desc, 
        bul.county,
	bul.postal,
	bul.REP_DIRECTOR,  
        bl.COMPANY_DESC, 
        jc.CUST_CODE,
        cl.NAME,
        cl.REP_DIRECTOR,
        cl.BUSINESS_TYPE,
        cl.BUSINESS_ITEM,
        cal.ADDRESS1,
        cal.ADDRESS2,
        cal.ADDRESS3,
        cal.ADDRESS4,
        cal.CITY,
        cal.COUNTY,
        cal.POSTAL]]></queryString>

